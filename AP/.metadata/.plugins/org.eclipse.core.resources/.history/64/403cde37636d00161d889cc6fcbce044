public class Bajaj extends TwoWheelerEngine{
	ThirdParty policy1 = new ThirdParty();
	public void set()
	{
		noOfWheels=2;
		model="Bajaj";
		ownerName="Owner1";
		policy1.setPolicy(2018,1111);
	}
	public void getOwnerName()
	{
		System.out.println(ownerName);
	}
	public void getModel()
	{
		System.out.println(model);
	}
}
